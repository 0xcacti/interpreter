{
  "grammars": [
    {
      "name": "monkey",
      "camelcase": "Monkey",
      "scope": "source.monkey",
      "path": ".",
      "file-types": ["monkey"],
      "injection-regex": "^monkey$",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for the Monkey programming language from Thorsten Bal",
    "authors": [
      {
        "name": "0xcacti"
      }
    ],
    "links": {
      "repository": "https://github.com/0xcacti/interpreter"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
